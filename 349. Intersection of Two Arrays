class Solution {
    public int[] intersection(int[] nums1, int[] nums2) {
        HashSet<Integer> set = new HashSet<>();
        HashSet<Integer> result = new HashSet<>();

        for(int num : nums1)
        {
            set.add(num);
        }
        for(int val : nums2)
        {
            if(set.contains(val))
            {
                result.add(val);
            }
        }
        int[] output = new int[result.size()];
        int i = 0;
        for(int ot : result)
        {
            output[i++] = ot; 
        }
        return output;
    }
}
